  "components": {
    "schemas": {
      "Materia": {
        "required": [
          "experiencia",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "type": "string"
          },
          "experiencia": {
            "maximum": 10,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "Resena": {
        "required": [
          "autorId",
          "fecha",
          "idResena",
          "puntuacion"
        ],
        "type": "object",
        "properties": {
          "idResena": {
            "type": "string"
          },
          "autorId": {
            "type": "string"
          },
          "puntuacion": {
            "maximum": 5,
            "minimum": 0,
            "type": "integer",
            "format": "int32"
          },
          "comentario": {
            "type": "string"
          },
          "fecha": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "Tutor": {
        "required": [
          "activo",
          "bio",
          "experiencia",
          "materias",
          "precioHora"
        ],
        "type": "object",
        "properties": {
          "activo": {
            "type": "boolean"
          },
